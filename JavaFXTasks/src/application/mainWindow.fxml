<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Label?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller" alignment="center" hgap="10" vgap="10">
	<Button text="Get Employee Names" onAction="#onButtonPressed" GridPane.rowIndex="0" GridPane.columnIndex="0" />
	<ListView fx:id="listView" GridPane.rowIndex="1" GridPane.columnIndex="0"></ListView>
	<ProgressBar fx:id="progressBar" GridPane.rowIndex="2" GridPane.columnIndex="0" visible="false"/>
	<Label fx:id="progressLabel" GridPane.rowIndex="3" GridPane.columnIndex="0" visible="false"/>
</GridPane>

